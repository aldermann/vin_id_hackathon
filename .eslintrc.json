{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb-base", "prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["warn", "double"],
        "indent": ["error", 4],
        "no-console": 0,
        "camelcase": 0
    }
}
